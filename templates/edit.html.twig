<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome Admin!{% endblock %}</title>
    {% block stylesheets %}
        <link  rel="stylesheet" type="text/css" media="screen" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"/>

    {% endblock %}
</head>
<body>
{% block body %}
{{ title }}
{{ description }}
{{ url }}
<div class="jumbotron d-flex  align-items-center">
    <div class="container ">
        <div class="form-group row ">

            <div class="col-sm-12 ">
                {%- block form -%}
                    {{ form_start(form) }}
                    {{ form_widget(form) }}
                    {{ form_end(form) }}
                {%- endblock form -%}
            </div>
        </div>
    </div>
</div>
{% endblock %}